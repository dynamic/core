{
  "name": "dynamic/core",
  "description": "The Dynamic Core Module",
  "authors": [
    {
      "name": "Dynamic",
      "homepage": "http://www.dynamicagency.com",
      "email": "dev@dynamicagency.com"
    }
  ],
  "keywords": [
    "silverstripe",
    "module"
  ],
  "license": "BSD-3-Clause",
  "type": "silverstripe-vendormodule",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.packagist.com/dynamic/"
    }
  ],
  "require": {
    "silverstripe/recipe-cms": "^4.0",
    "silverstripe/recipe-plugin": "^1.2",
    "silverstripe/vendor-plugin": "^1.0@dev",
    "dynamic/core-tools": "^4.0 || ^5.0",
    "dynamic/flexslider": "^3.0 || ^4.0",
    "silverstripe/tagfield": "^2.1",
    "silverstripe/userforms": "^5.2",
    "silverstripe/spamprotection": "^3.0",
    "unclecheese/display-logic": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.0",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^3.0",
    "phploc/phploc": "^4.0",
    "pdepend/pdepend": "^2.5",
    "theseer/phpdox": "^0.11.0"
  },
  "config": {
    "process-timeout": 600
  },
  "autoload": {
    "psr-4": {
      "Dynamic\\Core\\": "src/",
      "Dynamic\\Core\\Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-name": "dynamic-core",
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  }
}
